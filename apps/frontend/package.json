{
  "name": "voter-file-tool",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "next build",
    "dev": "next dev",
    "turbo": "next dev --turbo",
    "start": "next start",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:api": "jest --config jest.api.config.cjs",
    "test:api:watch": "jest --config jest.api.config.cjs --watch",
    "test:api:coverage": "jest --config jest.api.config.cjs --coverage",
    "dbconnect": "psql -h localhost -U matanAdmin -d postgres1",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "lint:fix": "eslint . --ext .js,.jsx,.ts,.tsx --fix",
    "format": "prettier --write \"./**/*.{js,jsx,ts,tsx,json,css,md}\"",
    "db_migrate": "prisma migrate dev && cd ../../ && pnpm --filter @voter-file-tool/shared-prisma build",
    "prismaLint": "prisma format",
    "sync-prisma": "node ../../scripts/sync-prisma-models.js",
    "postinstall": "prisma generate && cd ../../ && pnpm --filter @voter-file-tool/shared-prisma build && pnpm --filter @voter-file-tool/shared-validators build"
  },
